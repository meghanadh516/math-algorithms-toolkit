from algorithms.matrix import add_matrices, transpose_matrix, multiply_matrices

def test_add_matrices():
    A = [[1,2],[3,4]]
    B = [[5,6],[7,8]]
    assert add_matrices(A,B) == [[6,8],[10,12]]

def test_transpose():
    A = [[1,2],[3,4]]
    assert transpose_matrix(A) == [[1,3],[2,4]]

def test_multiply():
    A = [[1,2],[3,4]]
    B = [[5,6],[7,8]]
    assert multiply_matrices(A,B) == [[19,22],[43,50]]

